{
  "name": "capacitor-passkey-plugin",
  "version": "1.0.0",
  "description": "Cross-platform WebAuthn (Passkeys) plugin for Capacitor",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "typesVersions": {
    "*": {
      "adapter": [
        "dist/esm/adapter.d.ts"
      ],
      "storage": [
        "dist/esm/storage.d.ts"
      ]
    }
  },
  "unpkg": "dist/plugin.js",
  "homepage": "https://github.com/lux2983/capacitor-passkey-plugin/blob/main/README.md",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/plugin.cjs.js"
    },
    "./adapter": {
      "types": "./dist/esm/adapter.d.ts",
      "import": "./dist/esm/adapter.js"
    },
    "./storage": {
      "types": "./dist/esm/storage.d.ts",
      "import": "./dist/esm/storage.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Sources",
    "Package.swift",
    "CapacitorPasskeyPlugin.podspec",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "author": "Argo Navis Dev <info@argo-navis.dev>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lux2983/capacitor-passkey-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/lux2983/capacitor-passkey-plugin/issues"
  },
  "keywords": [
    "capacitor",
    "capacitor-plugin",
    "passkey",
    "webauthn",
    "fido2",
    "authentication",
    "ios",
    "android"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "xcodebuild -scheme PasskeyPlugin -destination generic/platform=iOS",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\" --plugin=prettier-plugin-java",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api PasskeyPlugin --output-readme README.md --output-json dist/docs.json --project tsconfig.docgen.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.mjs",
    "test": "vitest run --passWithNoTests",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/docgen": "^0.3.1",
    "@capacitor/ios": "^8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@ionic/eslint-config": "^0.4.0",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^2.0.0",
    "@simplewebauthn/browser": "^13.2.2",
    "eslint": "^8.57.1",
    "prettier": "^3.6.2",
    "prettier-plugin-java": "^2.7.7",
    "rimraf": "^6.1.0",
    "rollup": "^4.53.2",
    "swiftlint": "^2.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "@capacitor/core": ">=8.0.0",
    "@capacitor/preferences": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "@capacitor/preferences": {
      "optional": true
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
